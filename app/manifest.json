{
    "name": "Datamatic App",
    "description": "MOHAnet - Datamatic App",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "icons": {
        {
	"src": "icons/icons32.png",
	"sizes":"32x32",
	"type":"image/png"	
	},
 	{
	"src": "icons/icons128.png",
	"sizes":"128x128",
	"type":"image/png"	
	},
 	{
	"src": "icons/icons512.png",
	"sizes":"512x512",
	"type":"image/png"	
	},      
    },
    "permissions": [
      "storage"
  ], 
"start_url": "/#!/",
  "display": "standalone"
}